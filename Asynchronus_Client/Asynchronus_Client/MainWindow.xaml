<Window x:Class="Asynchronus_Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Asynchronus_Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Register" HorizontalAlignment="Center" Margin="0,271,0,0" VerticalAlignment="Top" Height="23" Width="100"/>
        <TextBox HorizontalAlignment="Center" Margin="0,147,0,0" Text="Login" TextWrapping="Wrap" VerticalAlignment="Top" Width="248" Height="18"/>
        <TextBox HorizontalAlignment="Center" Margin="0,170,0,0" Text="Password" TextWrapping="Wrap" VerticalAlignment="Top" Width="248" TextChanged="TextBox_TextChanged"/>
        <Button Content="Login" HorizontalAlignment="Center" VerticalAlignment="Top" Height="43" Width="188" Click="Button_Click" Margin="0,208,0,0"/>

    </Grid>
</Window>
